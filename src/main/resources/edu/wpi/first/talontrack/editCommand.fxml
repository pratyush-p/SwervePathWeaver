<GridPane fx:id="properties" hgap="5.0" vgap="5.0" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.first.talontrack.EditCommandController">
    <columnConstraints>
        <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" percentWidth="12.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" percentWidth="12.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" percentWidth="12.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <BorderPane.margin>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </BorderPane.margin>
    <Label text="Start Time (Cyan)" fx:id="lStart" />
    <Label text="Finish Time (Blue)" GridPane.rowIndex="1" fx:id="lFinish" />
    <Label text="P1" GridPane.columnIndex="2" fx:id="l1" />
    <Label text="P2" GridPane.columnIndex="2" GridPane.rowIndex="1" fx:id="l2" />
    <Label text="Name" GridPane.rowIndex="2" fx:id="lName" />
    <Label text="P4" GridPane.columnIndex="4" fx:id="l4" />
    <Label text="P5" GridPane.columnIndex="4" GridPane.rowIndex="1" fx:id="l5" />
    <Label text="P3" GridPane.columnIndex="2" GridPane.rowIndex="2" fx:id="l3" />
    <Label text="P6" GridPane.columnIndex="4" GridPane.rowIndex="2" fx:id="l6" />
    <TextField GridPane.columnIndex="1" fx:id="start" />
    <TextField fx:id="finish" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <TextField fx:id="p1" GridPane.columnIndex="3" />
    <TextField fx:id="p2" GridPane.columnIndex="3" GridPane.rowIndex="1" />
    <TextField fx:id="p4" GridPane.columnIndex="5" />
    <TextField fx:id="p5" GridPane.columnIndex="5" GridPane.rowIndex="1" />
    <TextField fx:id="pointName" GridPane.columnIndex="1" GridPane.rowIndex="2" />
    <TextField fx:id="p3" GridPane.columnIndex="3" GridPane.rowIndex="2" />
    <TextField fx:id="p6" GridPane.columnIndex="5" GridPane.rowIndex="2" />
</GridPane>